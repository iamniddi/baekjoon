   "source": [
    "N=int(input())\n",
    "list0=list(map(int, input().split()))\n",
    "bigNum=0\n",
    "sum=0\n",
    "for i in range(N):\n",
    "    if(list0[i]>bigNum):\n",
    "        bigNum=list0[i]\n",
    "for i in range(N):\n",
    "    list0[i]=list0[i]/bigNum*100\n",
    "for i in range(N):\n",
    "    sum+=list0[i]\n",
    "print(sum/N)"
   ]
  }
