   "source": [
    "H, M=map(int, input().split())\n",
    "time = int(input())\n",
    "a= time//60\n",
    "b= time%60\n",
    "if(M+b>59):\n",
    "    H+=1\n",
    "    M=M+b-60\n",
    "else:\n",
    "    M+=b\n",
    "if(H+a>23):\n",
    "    H=H+a-24\n",
    "else:\n",
    "    H+=a\n",
    "print(H,M)"
   ]
  }
