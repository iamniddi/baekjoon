   "source": [
    "output = 0\n",
    "N = int(input())\n",
    "for i in range(1, N+1):        \n",
    "    sum=0\n",
    "    list1 = list(map(int, str(i)))  \n",
    "    for q in list1:\n",
    "        sum+=q\n",
    "    sum+=i\n",
    "    if(sum == N):                 \n",
    "        output = i                   \n",
    "        break\n",
    "\n",
    "print(output)"
   ]
